{
  "compilerOptions": {
    "outDir": "./dist/",
    "esModuleInterop": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noImplicitAny": true
  },
  "exclude": ["spec"]
}
